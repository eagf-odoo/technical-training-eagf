<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data noupdate="1">
        <!-- Declare a sequence to set the registry number -->
        <record id="sequence_registry" model="ir.sequence">
            <field name="name">Registry Number</field>
            <field name="code">registry.number</field>
            <field name="prefix">MRN</field>
            <field name="padding">4</field>
            <field name="number_next">4</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>